{
  "name": "las-js",
  "version": "3.0.1",
  "description": "A zero-dependency JS library for reading/parsing .Las files",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "jest",
    "build": "./node_modules/.bin/tsc",
    "test:watch": "jest --watch",
    "lint": "./node_modules/.bin/tslint src/index.ts --fix"
  },
  "keywords": [
    "Las",
    "Geophysics",
    "Well-logs"
  ],
  "author": "Ikechukwu Eze <iykekings36@gmail.com> (https://github.com/iykekings)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/iykekings/las-js"
  },
  "homepage": "https://github.com/iykekings/las-js",
  "devDependencies": {
    "@types/jest": "^24.0.18",
    "husky": "^3.0.4",
    "jest": "^24.9.0",
    "lint-staged": "^9.2.3",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.3.0",
    "tslint": "^5.19.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.5.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm test"
    }
  },
  "lint-staged": {
    "*.js": [
      "npm run lint",
      "git add"
    ]
  }
}
